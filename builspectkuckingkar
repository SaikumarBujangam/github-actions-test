DROP TABLE IF EXISTS ${cicdvar:schema_name}.file_max_ts
(
	file_name				 varchar(50) not null ,
	row_count				 integer null ,
	max_ts                   timestamp null
);

DROP TABLE IF EXISTS ${cicdvar:schema_name}.file_max_ts_duplicate
(
	file_name				 varchar(50) not null ,
	row_count				 integer null ,
	max_ts                   timestamp null
);

DROP TABLE IF EXISTS ${cicdvar:schema_name}.file_max_ts_triplicate
(
	file_name				 varchar(50) not null ,
	row_count				 integer null ,
	max_ts                   timestamp null
);
