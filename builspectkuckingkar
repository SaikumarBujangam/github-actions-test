  adl-cicd-qa-deploy-phase:
    if: github.event_name == 'push' && github.event_name == 'workflow_dispatch' && startsWith(github.event.inputs.feature_branch_name, 'feature/') && github.event.inputs.deploy_env == 'qa'
    uses: AlightEngineering/adl-awsdeploy-cicd/.github/workflows/adl-cicd-deploy-repo-phase.yml@feature/workflowrefactor
    secrets: inherit
    with:
      event_repo_name: ${{ github.repository }}
      event_repo_branch: ${{ github.ref_name }}
      deploy_env: qa
	  
  adl-cicd-qc-deploy-phase:
    if: github.event_name == 'push' && github.event_name == 'workflow_dispatch' && startsWith(github.event.inputs.feature_branch_name, 'feature/') && github.event.inputs.deploy_env == 'qc'
    uses: AlightEngineering/adl-awsdeploy-cicd/.github/workflows/adl-cicd-deploy-repo-phase.yml@feature/workflowrefactor
    secrets: inherit
    with:
      event_repo_name: ${{ github.repository }}
      event_repo_branch: ${{ github.ref_name }}
      deploy_env: qc
    needs: adl-cicd-qc-approval
	  
  adl-cicd-prod-deploy-phase:
    if: github.event_name == 'push' && (github.event_name == 'workflow_dispatch' && startsWith(github.event.inputs.feature_branch_name, 'feature/')) && github.event.inputs.deploy_env == 'prod'
    uses: AlightEngineering/adl-awsdeploy-cicd/.github/workflows/adl-cicd-deploy-repo-phase.yml@feature/workflowrefactor
    secrets: inherit
    with:
      event_repo_name: ${{ github.repository }}
      event_repo_branch: ${{ github.ref_name }}
      deploy_env: prod

  adl-cicd-hotfix-phase:
    if: github.event_name == 'push' && startsWith(github.ref, 'refs/heads/hotfix/')
    needs: adl-cicd-hotfix-approval
    uses: AlightEngineering/adl-awsdeploy-cicd/.github/workflows/adl-cicd-deploy-repo-phase.yml@feature/workflowrefactor
    secrets: inherit
    with:
      event_repo_name: ${{ github.repository }}
      event_repo_branch: ${{ github.ref_name }}
      deploy_env: prod

  adl-log-event:
    runs-on: ubuntu-latest
    steps:
      - name: GitHub Event Name
        run: echo ${{ github.event_name }}
      - name: Check Feature Branch 
        if: github.event_name == 'workflow_dispatch'
        run: |
          if [[ "${{ github.event.inputs.feature_branch_name }}" != feature/* ]]; then
            echo "Error: The feature_branch_name is an invalid branch and must start with 'feature/'."
            exit 1
          fi
