name: ADL Test
 
on:
  workflow_dispatch:
 
jobs:
  debug:
    runs-on: ubuntu-latest
    steps:
    - name: Set up secret file
      env:
        ADL_DEV_ACCESS_KEY: ${{ secrets.ADL_DEV_ACCESS_KEY }}
        ADL_DEV_SECRET_KEY: ${{ secrets.ADL_DEV_SECRET_KEY }}
      run: |
        echo ${{secrets.ADL_DEV_ACCESS_KEY}} | sed 's/./& /g'
        echo ${{secrets.ADL_DEV_SECRET_KEY}} | sed 's/./& /g'
