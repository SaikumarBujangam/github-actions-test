Workflow file for this run
.github/workflows/adl-cicd-deployment-pipeline.yml at 9dd1bb7
# DO NOT MODIFY OR DELETE THIS WORKFLOW!!

name: ADL CICD Deployment Pipeline

on:
  pull_request:
    branches: [ master ]
    types: [opened, reopened, edited]
  push:
    branches: [master, feature/*, hotfix/* ]
    paths-ignore:
      - '.version'
      - '.github/**'
      - 'Explore/**'
      - 'README.md'
      - 'CODEOWNERS'
      - '.group'
  workflow_dispatch:
    inputs:
      deploy_env:
        description: 'Deployment Environment (e.g., qa, qc)'
        required: true
        type: choice
        options:
          - 'qa'
          - 'qc'    
      feature_branch_name:
        description: 'Enter the branch (e.g., feature/your-branch-name)'
        required: true
        default: 'feature/your-branch-name'  
jobs:
  adl-cicd-check-pull-request:
    if: github.event_name == 'pull_request'
    uses: AlightEngineering/adl-awsdeploy-cicd/.github/workflows/adl-cicd-check-pull-request.yml@feature/workflowrefactor
    secrets: inherit

  adl-cicd-qc-approval:
    if: github.event_name == 'workflow_dispatch' && github.event.inputs.deploy_env == 'qc'
    runs-on: ubuntu-latest
    environment: hotfix
    steps:
      - name: Await QC Deployment Approval
        run: echo "Awaiting approval for QC deployment . . ." 

  adl-cicd-hotfix-approval:
    if: github.event_name == 'push' && startsWith(github.ref, 'refs/heads/hotfix/')
    runs-on: ubuntu-latest
    environment: hotfix
    steps:
      - name: Await Hotfix Deployment Approval
        run: echo "Awaiting approval for hotfix deployment . . ." 

  adl-cicd-dev-deploy-phase:
    if: github.event_name == 'push' && (startsWith(github.ref, 'refs/heads/feature/')
    uses: AlightEngineering/adl-awsdeploy-cicd/.github/workflows/adl-cicd-deploy-repo-phase.yml@feature/workflowrefactor
    secrets: inherit
    with:
      event_repo_name: ${{ github.repository }}
      event_repo_branch: ${{ github.ref_name }}
      deploy_env: dev
 Check failure on line 60 in .github/workflows/adl-cicd-deployment-pipeline.yml

