#!/bin/bash

# Redirect output to a log file
logfile="/path/to/execution_time.log"

# Function to log command execution time
log_execution_time() {
    echo "Command: $1" >> "$logfile"
    { time $1 ; } 2>> "$logfile"
    echo -e "\n----------------------------------------\n" >> "$logfile"
}

# Your commands
log_execution_time "chown adl_dev_all_mig_svc:adl_all_mig /apps/dataproduct/cases"
log_execution_time "chmod -R 755 /apps/dataproduct/cases"
log_execution_time "chown -R adl_dev_all_mig_svc:adl_all_mig /apps/dataproduct/cases/adl-cases-hrxassist-etl"

# Add the rest of your commands using the log_execution_time function

# Example:
log_execution_time "ls -al /apps/dataproduct/cases/adl-cases-hrxassist-etl"
log_execution_time "sudo -i -u adl_dev_all_mig_svc mkdir /apps/dataproduct/cases/adl-cases-hrxassist-etl/1.0.65"
log_execution_time "sudo -i -u adl_dev_all_mig_svc mkdir -p /apps/dataproduct/cases/adl-cases-hrxassist-etl/land/stage"
log_execution_time "sudo -i -u adl_dev_all_mig_svc tar -xvf /apps/dataproduct/cases/adl-cases-hrxassist-etl/land/adl-cases-hrxassist-etl-1.0.65.tar --directory /apps/dataproduct/cases/adl-cases-hrxassist-etl/land/stage"

# Continue with the rest of your commands

# Example:
log_execution_time "cd /apps/dataproduct/cases/adl-cases-hrxassist-etl/land/stage"
log_execution_time "ls -al /apps/dataproduct/cases/adl-cases-hrxassist-etl/land/stage"

# ... rest of your script

# After the script finishes, you can check the logfile for execution times
