import math

# Define parent classes for each operation
class SquareRoot:
    def sqroot(self, x):
        return math.sqrt(x)

class Addition:
    def add(self, x, y):
        return x + y

class Subtraction:
    def subtract(self, x, y):
        return x - y

class Multiplication:
    def multiply(self, x, y):
        return x * y

class Division:
    def divide(self, x, y):
        if y == 0:
            raise ValueError("Cannot divide by zero.")
        return x / y

# Define child class that inherits from all the above classes
class MathNew(SquareRoot, Addition, Subtraction, Multiplication, Division):
    def __init__(self):
        # Initialize all parent classes
        super(SquareRoot, self).__init__()
        super(Addition, self).__init__()
        super(Subtraction, self).__init__()
        super(Multiplication, self).__init__()
        super(Division, self).__init__()

    # You can also define new methods or override existing ones here

# Example usage:
math_new = MathNew()

# Test the methods
print(math_new.sqroot(16))         # Should print 4.0
print(math_new.add(5, 3))          # Should print 8
print(math_new.subtract(5, 3))     # Should print 2
print(math_new.multiply(5, 3))     # Should print 15
print(math_new.divide(5, 3))       # Should print approximately 1.666...
