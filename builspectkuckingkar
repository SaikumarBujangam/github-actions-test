class LengthString:
    def __init__(self, string):
        self.string = string

    def __gt__(self, other):
        return len(self.string) > len(other.string)

# Get strings from user input
input_string1 = input("Enter the first string: ")
input_string2 = input("Enter the second string: ")

# Create LengthString instances
string1 = LengthString(input_string1)
string2 = LengthString(input_string2)

# Compare based on length
if string1 > string2:
    print(f'"{input_string1}" is longer than "{input_string2}"')
elif string1 < string2:
    print(f'"{input_string1}" is shorter than "{input_string2}"')
else:
    print(f'"{input_string1}" and "{input_string2}" are of the same length')
