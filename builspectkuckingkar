#!/bin/bash

PKG_CDP_BUILD_PATH="/apps/dataproduct"
PKG_CDP_BUILD_ZIP_NAME=cdp_pkg_deploy.zip
PKG_CDP_BUILD_USER_NAME=adl_${cdp_mig_mdsh_env}_all_mig_svc
if [ "$APP_ENV" == "prd" ]; then
    PKG_CDP_BUILD_GROUP_NAME="adl_all_mig"
else
    PKG_CDP_BUILD_GROUP_NAME="adl_${cdp_mig_mdsh_env}_all_etl_runtime"
fi
PKG_CDP_BUILD_AFTER_INSTALL_TEMPLATE=${APP_ENV}_pkg_cdp_build_afterinstall.template

CDP_BUILD_PATH="/apps/dataproduct"	
CDP_BUILD_ZIP_NAME=cdp_deploy.zip
CDP_BUILD_USER_NAME=adl_${cdp_mig_mdsh_env}_all_mig_svc
if [ "$APP_ENV" == "prd" ]; then
    PKG_CDP_BUILD_GROUP_NAME="adl_all_mig"
else
    PKG_CDP_BUILD_GROUP_NAME="dl_${cdp_mig_mdsh_env}_all_etl_runtime"
fi
CDP_BUILD_AFTER_INSTALL_TEMPLATE=${APP_ENV}_cdp_build_afterinstall.template
